{
    "name": "Youtube-Notify",
    "version": "1.0",
    "description": "Alerts you whenever a new trailer or music video comes out!",
    "permissions": ["storage"],
    "page_action": {
        "default_popup": "homepage.html"
    },
    "background": {
        "page": "background.html",
        "persistent": false
    },
    "manifest_version": 2,
    "content_security_policy": "script-src 'self' https://smtpjs.com/v3/smtp.js https://code.jquery.com/jquery-3.4.1.min.js; object-src 'self'"

  }