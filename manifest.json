{
    "name": "Youtube-Notify",
    "version": "1.0",
    "description": "Alerts you whenever a new trailer or music video comes out!",
    "permissions": ["storage"],
    "background": {
        "page": "background.html",
        "persistent": false
    },
    "manifest_version": 2,
    "content_security_policy": "script-src 'self' https://smtpjs.com/v3/smtp.js; object-src 'self'"
  }