{
    "name": "Youtube-Notify",
    "version": "1.0",
    "description": "Alerts you whenever a new trailer or music video comes out!",
    "permissions": ["declarativeContent", "storage"],
    "page_action": {
        "default_popup": "main.html",
        "default_icon": {
            "16": "images/get_started16.png",
            "32": "images/get_started32.png",
            "48": "images/get_started48.png",
            "128": "images/get_started128.png"
          }
    },
    "icons": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
      },
    "background": {
        "page": "background.html",
        "persistent": false
    },
    "manifest_version": 2,
    "content_security_policy": "script-src 'self' https://smtpjs.com/v3/smtp.js https://code.jquery.com/jquery-3.4.1.min.js; object-src 'self'"

  }